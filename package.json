{
  "name": "findconnect",
  "version": "1.0.0",
  "description": "A comprehensive missing persons and emergency services application",
  "main": "index.js",
  "scripts": {
    "start": "node server/src/server.js",
    "server": "nodemon server/src/server.js",
    "client": "cd client && npm start",
    "mobile": "cd mobile && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install && cd ../mobile && npm install"
  },
  "keywords": [
    "missing persons",
    "emergency services",
    "fined connect"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.5.0",
    "nodemon": "^3.1.4",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "socket.io-client": "^4.7.5",
    "styled-components": "^6.1.11"
  }
}
